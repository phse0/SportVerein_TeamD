<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="data.model.Match" table="match">
        
        <id name="matchID" column="matchID" type="integer">
            <generator class="increment" />
        </id>
        
        <many-to-one class="data.model.Tournament" column="tournamentID" name="tournament" />
        <many-to-one class="data.model.Team" column="team1ID" name="team1" />
        <many-to-one class="data.model.TEam" column="team2ID" name="team2" />
        
    </class>
  

</hibernate-mapping>
